{
  "app": "etracker",
  "privateKeyEnvVar": "ETRACKER_PRIVATE_KEY",
  "didEnvVar": "ETRACKER_DID",
  "port": 8080,
  "js": null,
  "domainBase": "",
  "cloudflare": true,
  "createDB": false,
  "caches": null,
  "topics": null,
  "queues": null,
  "buckets": null,
  "secrets": [
    {
      "name": "ETRACKER_METRICS_AUTH_TOKEN",
      "variable": true
    },
    {
      "name": "ETRACKER_ADMIN_DASHBOARD_USER",
      "variable": true
    },
    {
      "name": "ETRACKER_ADMIN_DASHBOARD_PASSWORD",
      "variable": true
    }
  ],
  "tables": [
    {
      "name": "egress-records",
      "attributes": [
        {
          "name": "batch",
          "type": "S"
        },
        {
          "name": "unprocessedSince",
          "type": "S"
        }
      ],
      "hashKey": "batch",
      "rangeKey": "",
      "globalSecondaryIndexes": {
        "unprocessed": {
          "name": "unprocessed",
          "hashKey": "batch",
          "rangeKey": "unprocessedSince",
          "projectionType": "INCLUDE",
          "nonKeyAttributes": [
            "node",
            "cause"
          ]
        }
      }
    },
    {
      "name": "consolidated-records",
      "attributes": [
        {
          "name": "cause",
          "type": "S"
        },
        {
          "name": "node",
          "type": "S"
        },
        {
          "name": "processedAt",
          "type": "S"
        }
      ],
      "hashKey": "cause",
      "rangeKey": "",
      "globalSecondaryIndexes": {
        "node-stats": {
          "name": "node-stats",
          "hashKey": "node",
          "rangeKey": "processedAt",
          "projectionType": "INCLUDE",
          "nonKeyAttributes": [
            "totalEgress"
          ]
        }
      }
    },
    {
      "name": "space-stats",
      "attributes": [
        {
          "name": "space",
          "type": "S"
        },
        {
          "name": "date",
          "type": "S"
        }
      ],
      "hashKey": "space",
      "rangeKey": "date"
    }
  ],
  "networks": [
    "warm"
  ],
  "writeToContainer": false
}